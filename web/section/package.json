    "dotenv": "^16.3.1",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "patch-package": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^13.2.2",
    "single-spa": "^5.9.3",
    "single-spa-react": "^4.3.1",
    "styled-components": "^6.0.7",
    "thejuanbm-react-library-ui": "^0.0.15"
  },
  "types": "dist/microfrontend-with-react-section.d.ts"
